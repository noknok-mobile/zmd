mixin dictionary__item(letter, items)
    .dictionary__group.tile.js-filter-target(data-filter= letter)
        .dictionary__letter= letter
        .dictionary__item-list
            each item in items
                a.dictionary__item.link(href="#")= item

//- 
.dictionary
    +filter(filterSection, "A-Ð¯")(class="dictionary-filter",itemClass= "dictionary-filter__item")

    each value, key in filterSection 
        +dictionary__item(key, value)


